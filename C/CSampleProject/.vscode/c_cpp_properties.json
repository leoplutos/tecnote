{
	"configurations": [
		{
			"name": "GCC",
			"includePath": [ //编译时-I参数后面指定的内容，需要加到这里
				"${workspaceFolder}/**",
				"${config:mingw.home}\\include",
				"${workspaceFolder}/include/**"
			],
			"defines": [
				"_DEBUG",
				"UNICODE",
				"_UNICODE"
			],
			"compilerPath": "${config:mingw.home}\\bin\\gcc.exe",
			"compilerArgs": [ //编译时-I参数后面指定的内容，需要加到这里
				"-I${config:mingw.home}\\include",
				"-I./include"
			],
			"cStandard": "c17",
			"cppStandard": "c++17",
			"intelliSenseMode": "windows-gcc-x64"
		}
	],
	"version": 4
}
