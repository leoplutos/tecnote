	//-----------------------------------------------"
	//              Whitespace++插件设定             "
	//-----------------------------------------------"
	"whitespacepp.renderTAB": true, // 是否渲染 TAB (\t) 字符
	"whitespacepp.renderWS": true, //是否渲染宽空格字符
	"whitespacepp.renderLF": true, //是否渲染换行 (\n) 字符。
	"whitespacepp.renderEOF": true, //是否在文件末尾渲染 [EOF] 标记
	"whitespacepp.textTAB": "┆", //TAB (\t) 的文本。建议指定1em大小的字符，也可以指定unicode字符，如“^”、“￫”、“‣”、“▸”、“▹”、“❯”。
	"whitespacepp.textWS": " ", //宽空格的文本。建议指定2em大小的字符，也可以指定unicode字符，如“▢”、“▭”、“◌”、“▨”、“▒”、“🔲”。
	"whitespacepp.textLF": "↵", //换行文本 (\n)。您还可以指定 unicode 字符，例如“↩”、“◂”、“▾”。
	"whitespacepp.textEOF": "[EOF]", //EOF（文件结尾）的文本。您还可以指定 unicode 字符。
	//暗色系 宽空格#FF6464 tab#444444 换行符#444444
	"whitespacepp.foreColorTAB": "#808080", //TAB (\t) 的文本颜色。
	"whitespacepp.foreColorWS": "#FF6464", //宽空格的文本颜色，设定和背景色trailing-spaces颜色一致
	"whitespacepp.foreColorLF": "#808080", //换行 (\n) 的文本颜色。
	//亮色系 宽空格rgba(255,100,100,0.15) tab#C0C0C0 换行符#C0C0C0  rgba(255,0,0,0.3)=#ff00004d  rgba(255,100,100,0.15)=#ff646426
	//"whitespacepp.foreColorTAB": "#808080", //TAB (\t) 的文本颜色。（备用#C0C0C0）
	//"whitespacepp.foreColorWS": "rgba(255,100,100,0.15)", //宽空格的文本颜色，设定和trailing-spaces颜色一致
	//"whitespacepp.foreColorLF": "#C0C0C0", //换行 (\n) 的文本颜色。
	"whitespacepp.foreColorEOF": "#00FFFF", //EOF（文件结尾）的文本颜色。
	"whitespacepp.changeBackColorTAB": false, //是否更改 TAB (\t) 的背景颜色。
	"whitespacepp.changeBackColorWS": true, //是否更改宽空格的背景颜色。
	"whitespacepp.changeBackColorEOF": true, //是否更改 EOF（文件结尾）的背景颜色。
	//"whitespacepp.backColorTAB": "", //TAB (\t) 的背景色。
	"whitespacepp.backColorWS": "rgba(255,0,0,0.3)", //宽空格的背景色。
	"whitespacepp.backColorEOF": "#000000", //EOF（文件结尾）的背景色。
	"whitespacepp.renderingDelayMs": 0, //渲染延迟时间，以毫秒为单位。间隔短于延迟时间的绘图请求将被取消。
	"whitespacepp.adjustWidthEOF": 110, //以 % 为单位调整 EOF 的背景宽度。textEOF通过乘以by的长度adjustWidthEOF再除以 100 来计算。
