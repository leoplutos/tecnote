{
	//-----------------------------------------------"
	//               基本设定                        "
	//-----------------------------------------------"
	//"editor.fontFamily": "Consolas, 'Courier New', monospace", //默认
	"editor.fontFamily": "'等距更纱黑体 SC Nerd Font light', '等距更纱黑体 SC light', '更紗等幅ゴシック J Nerd Font light', '更紗等幅ゴシック J light', 'MS Gothic', 'ＭＳ ゴシック', 'JetBrains Mono', 'Cascadia Code', 'Consolas', 'Courier New', monospace",
	"editor.fontSize": 16, //设置字体大小
	"editor.fontWeight": "400", //设置字体粗细，可选normal,bold,100到1000之间的数字(400为默认)，选择合适的就行
	"editor.fontLigatures": true, //设置启用连字
	//"editor.fontVariations": "'Ligconsolata' 0.5",
	"editor.formatOnSave": false, //在保存时格式化文件
	//"editor.letterSpacing": 0.1, //字母间距
	"editor.lineHeight": 22, //行高
	"editor.minimap.enabled": true, //控制是否显示缩略图(右侧的minimap)
	"editor.minimap.autohide": true, //控制是否自动隐藏缩略图(右侧的minimap)
	"editor.tabSize": 4, //设定tab长度
	"editor.rulers": [
		100,
		200
	], // 垂直标尺
	"editor.detectIndentation": false, //不要通过打开文件来计算制表符大小
	"editor.renderWhitespace": "selection", //设定显示tab和空格，selection为仅选中内容显示。未选中的内容使用[Whitespace++]插件显示
	"editor.renderControlCharacters": true, //设定显示控制字符
	"editor.renderLineHighlight": "all", //突出显示导航线和当前行
	"editor.selectionHighlight": true, // 控制编辑器是否应突出显示与所选内容类似的匹配项。
	"editor.occurrencesHighlight": true, // 控制编辑器是否突出显示语义符号的匹配项。
	"editor.insertSpaces": false, //按下tab时不替换为空格
	"editor.autoClosingBrackets": "beforeWhitespace", //括号和引号自动补全，光标在空白前面的时候自动补全
	"editor.autoClosingQuotes": "beforeWhitespace",
	"editor.autoSurround": "languageDefined",
	"editor.quickSuggestions": {
		// 自动补全设定
		"other": "on",
		"comments": "off",
		"strings": "off"
	},
	"editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?·～！￥…（）—【】、；：‘’“”，。《》・「」＜＞＋－＊／｛｝｜～＆％＄＃　？ ", //字符分割,双击选中一串字符序列时，VSCode就会依此来区分词
	"editor.suggest.localityBonus": true, //自动补全时显示离光标近的对象
	"editor.find.loop": false, //控制在找不到其他匹配项时，不自动从开头(或结尾)重新开始搜索。
	"editor.inlayHints.enabled": "offUnlessPressed", //在编辑器中启用内嵌提示。on/onUnlessPressed/offUnlessPressed/off，offUnlessPressed为默认不表示按下Ctrl+Alt表示
	"editor.guides.indentation": false, //缩进参考线
	"editor.guides.bracketPairs": false, //控制是否启用括号对指南。true: 启用括号对参考线。active: 仅为活动括号对启用括号对参考线。 false: 禁用括号对参考线。
	"editor.bracketPairColorization.enabled": false, //控制是否启用括号对着色
	"editor.unicodeHighlight.invisibleCharacters": false, //禁用不可见突出
	"editor.multiCursorModifier": "alt", //多光标选择模式（Ctrl 或者 Alt）
	"editor.cursorSurroundingLines": 4, // 控制光标周围可见行
	"editor.cursorSurroundingLinesStyle": "all", // 始终强制执行
	"editor.semanticHighlighting.enabled": true, //开启基于LSP语义的高亮
	"editor.unicodeHighlight.nonBasicASCII": false, //控制是否突出显示所有非基本 ASCII 字符
	//"window.menuBarVisibility": "toggle", //菜单处于隐藏状态，但通过按 Alt 键可在窗口顶部显示。
	"window.title": "${dirty}${activeEditorLong}${separator}${rootName}${separator}${appName}", //让标题能够显示完整的文件路径名
	//"window.zoomLevel": 0.4, //缩放比例
	"window.newWindowDimensions": "maximized", //总是最大化打开窗口
	"explorer.compactFolders": false, // 资源管理器不采用紧凑模式
	"files.autoSave": "off", //取消自动保存
	"files.eol": "auto", //自动设定换行符，也可以设定 "\n"
	"files.autoGuessEncoding": true, //自动识别文字编码
	//"files.trimTrailingWhitespace": true, // 自动删除文件行尾空格
	"files.exclude": { // 编辑器窗口忽略文件夹
		//"somefolder/": true,
		//"somefile": true
	},
	"search.exclude": { // 搜索结果中忽略文件夹
		//"someFolder/": true,
		//"somefile": true
	},
	"files.associations": {
		"*.pc": "c" //设定.pc扩展名关联为c程序
	},
	"security.workspace.trust.untrustedFiles": "open", //6.10号版本的VS Code加了个新功能: Workspace Trust就是会在你每打开一个文件夹的时候就询问是否信任
	"update.mode": "manual", //禁止自动升级VSCode版本
	"outline.showVariables": false, //outline不显示变量
	"search.mode": "newEditor", // 在新的编辑器中搜索
	"search.showLineNumbers": true, // 显示搜索结果所在行号
	"search.smartCase": true, // 若搜索词全为小写，则不区分大小写进行搜索，否则区分大小写进行搜索。
	"workbench.editor.enablePreview": false, // 全局禁用预览模式
	"workbench.editor.showTabs": true, //每次都新打开
	"workbench.editor.enablePreviewFromQuickOpen": false, //每次都新打开
	"workbench.editorAssociations": { //设定文件使用的默认编辑器
		"*.hex": "hexEditor.hexedit",
		"*.exe": "hexEditor.hexedit",
		"*.svg": "hediet.vscode-drawio-text",
	},
	//"workbench.editor.tabSizing": "fixed", //tab宽度自适应
	//"workbench.list.horizontalScrolling": true, //资源管理器水平滚动
	"zenMode.centerLayout": false, //禅模式取消居中
	"zenMode.hideLineNumbers": false, //禅模式显示行号
