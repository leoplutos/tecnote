{//这是一个Snippet代码片段配置文件，请放在User/snippets路径下
	// include导入系统头文件<>
	"include导入系统头文件<>": {
		"prefix": ["#", "#i", "#in", "#inc", "#incl", "#inclu"],
		"body": [
			"#include <${1:system.h}>"
		],
		"description": "include导入系统头文件<>"
	},
	// include导入用户头文件""
	"include导入用户头文件\"\"": {
		"prefix": ["#", "#i", "#in", "#inc", "#incl", "#inclu"],
		"body": [
			"#include \"${1:user.h}\""
		],
		"description": "include导入用户头文件\"\""
	},
	// 添加define定义
	"define定义": {
		"prefix": ["#", "#d", "#de", "#def", "#defi", "#defin"],
		"body": [
			"#define ${1:NAME} ${2:VALUE}"
		],
		"description": "define定义"
	},
	// 添加#ifdef定义语句块
	"添加#ifdef定义语句块": {
		"prefix": ["#", "#i", "#if", "#ifd", "#ifde"],
		"body": [
			"#ifdef ${1:DEBUG}",
			"\t$2",
			"#endif"
		],
		"description": "添加#ifdef定义语句块"
	},
	// 添加头文件模板语句块
	"添加头文件模板语句块": {
		"prefix": ["he", "hea", "head"],
		"body": [
			"#ifndef ${1:__FILENAME_H__}",
			"#define ${1:__FILENAME_H__}",
			"",
			"$2",
			"",
			"#endif // ${1:__FILENAME_H__}",
		],
		"description": "添加头文件模板语句块"
	},
	// if语句块
	"if语句块": {
		"prefix": ["if"],
		"body": [
			"if (${1:booltype}) {",
			"\t$2",
			"}"
		],
		"description": "if语句块"
	},
	// if-else语句块
	"if-else语句块": {
		"prefix": ["if", "ife", "ifel"],
		"body": [
			"if (${1:booltype}) {",
			"\t$2",
			"} else {",
			"\t$3",
			"}",
		],
		"description": "if-else语句块"
	},
	// if-else-if语句块
	"if-else-if语句块": {
		"prefix": ["if", "ife", "ifel", "ifeli", "ifelif"],
		"body": [
			"if (${1:booltype}) {",
			"\t$2",
			"} else if (${3:booltype}) {",
			"\t$4",
			"} else {",
			"\t$5",
			"}",
		],
		"description": "if-else-if语句块"
	},
	// switch语句块
	"switch语句块": {
		"prefix": ["sw", "swi", "swit", "switc", "switch"],
		"body": [
			"switch (${1:value}) {",
			"\tcase ${2:item}:",
			"\t\tbreak;",
			"\tdefault:",
			"\t\tbreak;",
			"}"
		],
		"description": "switch语句块"
	},
	// while语句块
	"while语句块": {
		"prefix": ["wh", "whi", "whil", "while"],
		"body": [
			"while (${1:booltype}) {",
			"\t$2",
			"\t${3:break;}",
			"}"
		],
		"description": "while语句块"
	},
	// for语句块
	"for语句块": {
		"prefix": ["fo", "for"],
		"body": [
			"int i;",
			"for (i = 0; i <= ${1:size}; i++) {",
			"\t$2",
			"}",
		],
		"description": "for语句块"
	},
	// 添加struct结构体
	"添加struct结构体": {
		"prefix": ["st", "str", "stru", "struc", "struct"],
		"body": [
			"struct ${1:name} {",
			"\t$2",
			"};",
		],
		"description": "添加struct结构体"
	},
	// 添加enum枚举
	"添加enum枚举": {
		"prefix": ["en", "enu", "enum"],
		"body": [
			"enum ${1:name} {",
			"\t$2",
			"};"
		],
		"description": "添加enum枚举"
	},
	// 添加function函数
	"添加function函数": {
		"prefix": ["fu", "fun", "func", "funct", "functi"],
		"body": [
			"int ${1:function_name}(int ${2:arguments}) {",
			"\t$3",
			"\treturn 0;",
			"}",
		],
		"description": "添加function函数"
	},
	// 添加main函数
	"添加main函数": {
		"prefix": ["ma", "mai", "main"],
		"body": [
			"#include <stdio.h>",
			"int main (int argc, char *argv[]) {",
			"\t$1",
			"\treturn 0;",
			"}",
		],
		"description": "添加main函数"
	},
	// 添加print语句
	"添加print语句": {
		"prefix": ["pr", "pri", "prin", "print"],
		"body": [
			"printf(\"[DEBUG] [%s at line:%d] - ${1:variable}:[%${2:s}] \\n\", __func__, __LINE__, ${1:variable});"
		],
		"description": "添加print语句"
	},
	// 打印sqlca信息（嵌入式SQL）
	"打印sqlca信息（嵌入式SQL）": {
		"prefix": ["pr", "pri", "prin", "print"],
		"body": [
			"printf(\"[DEBUG] [%s at line:%d] - sqlcode:[%ld] sqlstate:[%s] sqlerrm.sqlerrmc:[%s]\\n\", __func__, __LINE__, sqlca.sqlcode, sqlca.sqlstate, sqlca.sqlerrm.sqlerrmc);"
		],
		"description": "打印sqlca信息（嵌入式SQL）"
	},
	// 添加多行注释
	"添加多行注释": {
		"prefix": ["co", "com", "comm", "comme", "commen", "comment"],
		"body": [
			"/**************************************************************",
			" * System:         $1",
			" * Description:    $2",
			" * GreatDate:      $3",
			" * Author:         $4",
			" * UpdateDate:     $5",
			" **************************************************************/",
		],
		"description": "添加多行注释"
	},
	// 添加动态内存分配语句块(malloc)
	"添加动态内存分配语句块(malloc)": {
		"prefix": ["ma", "mal", "mall", "mallo", "malloc"],
		"body": [
			"#include <stdlib.h>",
			"#include <string.h>",
			"char *p_ptr;",
			"p_ptr = malloc(${1:frombufsize} + 1);",
			"if (p_ptr == NULL) {",
			"\tfprintf(stderr, \"[ERROR] [%s at line:%d] - FATAL:Out of memory!\\n\" , __func__, __LINE__);",
			"\treturn FAILED;",
			"}",
			"memcpy(p_ptr, ${2:frombuf}, ${1:frombufsize});",
			"p_ptr[${1:frombufsize}] = '\\0';",
			"",
			"${3://use p_ptr here}",
			"",
			"free(p_ptr);",
			"p_ptr = NULL;",
		],
		"description": "添加动态内存分配语句块(malloc)"
	},
}
